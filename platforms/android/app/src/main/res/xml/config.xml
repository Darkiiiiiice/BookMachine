<?xml version='1.0' encoding='utf-8'?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    id="com.ectongs"
    version="1.0.0"
    xmlns="http://www.w3.org/ns/widgets"
    xsi:schemaLocation="http://www.w3.org/ns/widgets ">
    <feature name="Whitelist">
        <param
            name="android-package"
            value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param
            name="onload"
            value="true" />
    </feature>
    <feature name="PrintPlugin">
        <param
            name="android-package"
            value="com.ectongs.plugin.PrintPlugin" />
        <param
            name="onload"
            value="true" />
    </feature>
    <feature name="SocketPlugin">
        <param
            name="android-package"
            value="com.ectongs.plugin.SocketPlugin" />
        <param
            name="onload"
            value="true" />
    </feature>
    <feature name="UartPlugin">
        <param
            name="android-package"
            value="com.ectongs.plugin.UartPlugin" />
        <param
            name="onload"
            value="true" />
    </feature>
    <name>EctongsAndroid</name>
    <description>
        Ectongs Android Application
    </description>
    <author
        email="dev@cordova.apache.org"
        href="http://cordova.io">
        Ectongs Company
    </author>
    <content src="index.html" />
    <access origin="*" />
    <!--<allow-intent href="http://*/*" />-->
    <!--<allow-intent href="https://*/*" />-->
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <preference
        name="loglevel"
        value="DEBUG" />
</widget>
